{
  "plugin_id": "analytics-plugin",
  "name": "Ecosystem Analytics Plugin",
  "version": "1.0.0",
  "author": "Agent-6 (Phase 3 Lead)",
  "description": "Real-time ecosystem analytics and metrics collection for Agent Cellphone V2",
  "category": "analytics",
  "dependencies": [],
  "permissions": [
    "read-analytics",
    "write-analytics",
    "read-system-info"
  ],
  "entry_point": "analytics_plugin:AnalyticsPlugin",
  "config_schema": {
    "collection_interval": {
      "type": "integer",
      "default": 60,
      "description": "Metrics collection interval in seconds"
    },
    "retention_days": {
      "type": "integer",
      "default": 7,
      "description": "How long to retain metrics data in days"
    },
    "enable_real_time": {
      "type": "boolean",
      "default": true,
      "description": "Enable real-time metrics collection"
    }
  },
  "min_core_version": "2.0.0",
  "max_core_version": null,
  "homepage": "https://github.com/agent-cellphone-v2/plugins/analytics",
  "repository": "https://github.com/agent-cellphone-v2/plugins/analytics",
  "license": "MIT",
  "tags": ["analytics", "metrics", "monitoring", "insights"]
}